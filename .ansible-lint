---
#
# Ansible managed
#
exclude_paths:
  - ./meta/preferences.yml
  - ./molecule/default/prepare.yml
  - ./molecule/default/converge.yml
  - ./molecule/default/verify.yml
  - ./molecule/default/collections.yml
  - ./.tox
  - ./.cache
  - ./.github
  - ./requirements.yml
skip_list:
  - yaml
  - role-name
  - experimental
  - line-length
  - fqcn-builtins
  - no-changed-when
  - meta-no-info
  - package-latest

warn_list:
  - name[template]
  - name[missing]

parseable: true
